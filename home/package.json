{
  "name": "home",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14 <17"
  },
  "scripts": {
    "start": "yarn concurrently -n \"server   ,tsc:watch\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm:server\" \"npm:tsc:watch\"",
    "server": "yarn webpack serve --env target=local",
    "build": "yarn webpack --env target=production",
    "build:cdn": "yarn build --output-path ../http/cdn/home",
    "build:dev": "yarn webpack --env target=local",
    "lint": "yarn eslint --ext .js,.jsx,.ts,.tsx --cache --quiet",
    "lint:fix": "yarn lint --fix",
    "prettier:fix": "prettier --write",
    "prettier": "prettier --check",
    "tsc": "tsc --noEmit",
    "tsc:watch": "yarn tsc --watch --pretty",
    "fix": "yarn lint:fix && yarn prettier:fix",
    "build:babel": "babel src/listing-creation --out-dir libx --extensions \".ts,.tsx\" --source-maps inline",
    "check:packages:es5": "are-you-es5 check ./ -a",
    "validate": "yarn tsc && yarn lint && yarn prettier"
  },
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "core-js": "3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.0",
    "react-router-dom": "^6.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.15.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.17.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.33.0",
    "are-you-es5": "^2.1.2",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "prettier": "^2.4.0",
    "react-refresh": "^0.11.0",
    "style-loader": "^3.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.0.3",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
